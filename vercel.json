{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "framework": null,
  "installCommand": "npm install && curl -L https://github.com/gohugoio/hugo/releases/download/v0.121.1/hugo_extended_0.121.1_Linux-64bit.tar.gz | tar -xz",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/default.aspx",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/Public/History/History_Home.aspx",
      "destination": "/about/history/",
      "permanent": true
    },
    {
      "source": "/Public/Golf/Golf_Home.aspx",
      "destination": "/events/golf/",
      "permanent": true
    },
    {
      "source": "/Public/Membership/Membership_Home.aspx",
      "destination": "/membership/",
      "permanent": true
    },
    {
      "source": "/Public/Freds/Freds_Home.aspx",
      "destination": "/services/freds-energy/",
      "permanent": true
    },
    {
      "source": "/Public/Discounts/Discounts_Home.aspx",
      "destination": "/services/discounts/",
      "permanent": true
    },
    {
      "source": "/Public/Links/Links_Home.aspx",
      "destination": "/links/",
      "permanent": true
    }
  ]
}